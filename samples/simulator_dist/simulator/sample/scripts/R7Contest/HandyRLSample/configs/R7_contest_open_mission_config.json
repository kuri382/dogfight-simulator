{
    "Manager":{
        "TimeStep":{"baseTimeStep":1.0,"defaultAgentStepInterval":1},
        "skipNoAgentStep":false,
        "Ruler":"R7ContestModels.R7ContestRuler",
        "Assets":{
            "type":"group",
            "Blue":{"type":"concatenate","elements":[
                {"type":"group","order":"fixed",
                    "names":["BlueF1","BlueF2","BlueF3","BlueF4"],
                    "elements":[
                        {"type":"alias","alias":"BlueFighters","instance":"Same","index":0},
                        {"type":"alias","alias":"BlueFighters","instance":"Same","index":1},
                        {"type":"alias","alias":"BlueFighters","instance":"Same","index":2},
                        {"type":"alias","alias":"BlueFighters","instance":"Same","index":3}
                    ],
                    "overrider":[
                        {"type":"group","order":"fixed","elements":[
                            {"type":"alias","alias":"BlueInitialState","instance":"Same","index":0},
                            {"type":"alias","alias":"BlueInitialState","instance":"Same","index":1},
                            {"type":"alias","alias":"BlueInitialState","instance":"Same","index":2},
                            {"type":"alias","alias":"BlueInitialState","instance":"Same","index":3}
                        ]},
                        {"type":"broadcast","number":4,
                        "element":{"type":"direct","value":{
                            "instanceConfig":{"datalinkName":"BlueDatalink"}
                        }}},
                        {"type":"group","order":"fixed","elements":[
                            {"type":"direct","value":{"Agent":{"type":"alias","alias":"BlueAgents","instance":"g1","index":0}}},
                            {"type":"direct","value":{"Agent":{"type":"alias","alias":"BlueAgents","instance":"g1","index":1}}},
                            {"type":"direct","value":{"Agent":{"type":"alias","alias":"BlueAgents","instance":"g1","index":2}}},
                            {"type":"direct","value":{"Agent":{"type":"alias","alias":"BlueAgents","instance":"g1","index":3}}}
                        ]}
                    ]
                },
                {"type":"group","order":"fixed",
                    "names":["BlueE"],
                    "elements":[{
                        "type":"alias","alias":"Escorted",
                        "overrider":[
                            {"type":"alias","alias":"BlueInitialState","index":4},
                            {"type":"direct","value":{
                                "Agent":{"type": "Internal", "model": "R7ContestModels.R7ContestBlueEscortedAgent", "name": "BlueE_Agent", "port": "0"},
                                "instanceConfig":{"datalinkName":"BlueDatalink"}
                            }}
                        ]
                    }]
                }
            ]},
            "Red":{"type":"concatenate","elements":[
                {"type":"group","order":"fixed",
                    "names":["RedF1","RedF2","RedF3","RedF4"],
                    "elements":[
                        {"type":"alias","alias":"RedFighters","instance":"Same","index":0},
                        {"type":"alias","alias":"RedFighters","instance":"Same","index":1},
                        {"type":"alias","alias":"RedFighters","instance":"Same","index":2},
                        {"type":"alias","alias":"RedFighters","instance":"Same","index":3}
                    ],
                    "overrider":[
                        {"type":"group","order":"fixed","elements":[
                            {"type":"alias","alias":"RedInitialState","instance":"Same","index":0},
                            {"type":"alias","alias":"RedInitialState","instance":"Same","index":1},
                            {"type":"alias","alias":"RedInitialState","instance":"Same","index":2},
                            {"type":"alias","alias":"RedInitialState","instance":"Same","index":3}
                        ]},
                        {"type":"broadcast","number":4,
                        "element":{"type":"direct","value":{
                            "instanceConfig":{"datalinkName":"RedDatalink"}
                        }}},
                        {"type":"group","order":"fixed","elements":[
                            {"type":"direct","value":{"Agent":{"type":"alias","alias":"RedAgents","instance":"g1","index":0}}},
                            {"type":"direct","value":{"Agent":{"type":"alias","alias":"RedAgents","instance":"g1","index":1}}},
                            {"type":"direct","value":{"Agent":{"type":"alias","alias":"RedAgents","instance":"g1","index":2}}},
                            {"type":"direct","value":{"Agent":{"type":"alias","alias":"RedAgents","instance":"g1","index":3}}}
                        ]}
                    ]
                },
                {"type":"group","order":"fixed",
                    "names":["RedE"],
                    "elements":[{
                        "type":"alias","alias":"Escorted",
                        "overrider":[
                            {"type":"alias","alias":"RedInitialState","index":4},
                            {"type":"direct","value":{
                                "Agent":{"type": "Internal", "model": "R7ContestModels.R7ContestRedEscortedAgent", "name": "RedE_Agent", "port": "0"},
                                "instanceConfig":{"datalinkName":"RedDatalink"}
                            }}
                        ]
                    }]
                }
            ]}
        },
        "CommunicationBuffers":{
            "BlueDatalink":{"participants":["PhysicalAsset:Blue/Blue[FE][0-9]*"]},
            "RedDatalink":{"participants":["PhysicalAsset:Red/Red[FE][0-9]*"]}
        },
        "AssetConfigDispatcher":{
            "Escorted":{"type":"PhysicalAsset","model":"R7ContestModels.R7ContestOpenEscorted"},
            "RedInitialState":{"type":"group","order":"fixed","elements":[
                {"type":"direct","value":{"instanceConfig":{
                    "pos":[10000,-100000,-10000],
                    "vel":270,
                    "heading":90
                }}},
                {"type":"direct","value":{"instanceConfig":{
                    "pos":[-10000,-100000,-10000],
                    "vel":270,
                    "heading":90
                }}},
                {"type":"direct","value":{"instanceConfig":{
                    "pos":[-20000,-100000,-10000],
                    "vel":270,
                    "heading":90
                }}},
                {"type":"direct","value":{"instanceConfig":{
                    "pos":[20000,-100000,-10000],
                    "vel":270,
                    "heading":90
                }}},
                {"type":"direct","value":{"instanceConfig":{
                    "pos":[0,-100000,-10000],
                    "vel":270,
                    "heading":90
                }}}
            ]},
            "BlueInitialState":{"type":"group","order":"fixed","elements":[
                {"type":"direct","value":{"instanceConfig":{
                    "pos":[-10000,100000,-10000],
                    "vel":270,
                    "heading":270
                }}},
                {"type":"direct","value":{"instanceConfig":{
                    "pos":[10000,100000,-10000],
                    "vel":270,
                    "heading":270
                }}},
                {"type":"direct","value":{"instanceConfig":{
                    "pos":[-20000,100000,-10000],
                    "vel":270,
                    "heading":270
                }}},
                {"type":"direct","value":{"instanceConfig":{
                    "pos":[20000,100000,-10000],
                    "vel":270,
                    "heading":270
                }}},
                {"type":"direct","value":{"instanceConfig":{
                    "pos":[0,100000,-10000],
                    "vel":270,
                    "heading":270
                }}}
            ]},
            "BlueFighters":{
                "type":"group","order":"fixed",
                "elements":[
                    {"type":"PhysicalAsset","model":"BlueFighter1"},
                    {"type":"PhysicalAsset","model":"BlueFighter2"},
                    {"type":"PhysicalAsset","model":"BlueFighter3"},
                    {"type":"PhysicalAsset","model":"BlueFighter4"}
                ]
            },
            "RedFighters":{
                "type":"group","order":"fixed",
                "elements":[
                    {"type":"PhysicalAsset","model":"RedFighter1"},
                    {"type":"PhysicalAsset","model":"RedFighter2"},
                    {"type":"PhysicalAsset","model":"RedFighter3"},
                    {"type":"PhysicalAsset","model":"RedFighter4"}
                ]
            }
        }
    }
}
